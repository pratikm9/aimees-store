{
	"name": "backend",
	"version": "1.0.0",
	"description": "Backend API for Aimees Store",
	"main": "server.js",
	"type": "module",
	"engines": {
		"node": "16.13.2"
	},
	"scripts": {
		"start": "node backend/server",
		"server": "nodemon backend/server",
		"client": "npm start --prefix frontend",
		"dev": "concurrently \" npm start\" \"npm run client\"",
		"data:import": "node backend/seeder",
		"data:destroy": "node backend/seeder -d",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
	},
	"author": "Pratik More",
	"license": "UNLICENSED",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-async-handler": "^1.2.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^6.7.0",
		"multer": "^1.4.5-lts.1",
		"validator": "^13.7.0"
	},
	"devDependencies": {
		"concurrently": "^7.5.0",
		"nodemon": "^2.0.20"
	}
}
